{
    "name": "{{ pluginName }}",
    "description": "{{ pluginDescription }}",
    "key": "{{ pluginKey }}",
    "baseUrl": "{{ localBaseUrl }}",
    "links": {
        "self": "{{ localBaseUrl }}{% url 'django-atlassian-confluence-connect-json' %}"
    },
    "vendor": {
        "name": "{{ vendorName }}",
        "url": "{{ vendorUrl }}"
    },
    {% if pluginEnableLicensing %}"enableLicensing": true,{% endif %}
    "authentication": {
        "type": "jwt"
    },
    "lifecycle": {
        "installed": "{% url 'django-atlassian-installed' %}",
        "uninstalled": "{% url 'django-atlassian-uninstalled' %}"
    },
    "scopes": {% if pluginScopes %}{{ pluginScopes|safe }}{% else %}["READ"]{% endif %},
    "apiVersion": 1,
    "version": {% if pluginVersion %}"{{pluginVersion}}"{% else %}"1.0.0"{% endif%},
    "modules": {{ modules|safe }}
}
